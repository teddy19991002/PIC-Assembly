                      00083 ;************************************************************************************
                      00084 ; Main - When SW2 is pressed, the LED is toggled, and the debounce routine for SW2 is
                      00085 ;        initiated.  Debouncing a switch is necessary to prevent one button press from 
                      00086 ;        being read as more than one button press by the microcontroller.  A 
                      00087 ;        microcontroller can read a button so fast that contact jitter in the switch 
                      00088 ;        may be interpreted as more than one button press.
                      00089 ;
                      00090 ;************************************************************************************
0011                  00091 Main
0011   1805           00092         btfsc   SW2                             ; Loop here until Switch 2 is pressed (SW2 is defined in
                             the include file)
0012   2811           00093         goto    Main
                      00094                                                         ; Once Switch 2 is pressed, toggle the LED on or
                             off
0013   3080           00095         movlw   0x80                    ; Exclusive ORing PORTD with 0x80 will effectively
0014   0688           00096         xorwf   PORTD,f                 ;  toggle RD7 on and off
                      00097 
0015                  00098 DebounceState1                          ; Wait here until SW2 is released
0015   1C05           00099         btfss   SW2
0016   2815           00100         goto    DebounceState1
0017   0181           00101         clrf    TMR0                    ; Once released clear TMR0 and the TMR0 interrupt flag
0018   110B           00102         bcf             INTCON,T0IF             ;  in preparation to time 16ms
                      00103 
0019                  00104 DebounceState2                          ; State2 makes sure than SW2 is unpressed for 16ms before
0019   1C05           00105         btfss   SW2                             ;  returning to look for the next time SW2 is pressed
001A   2815           00106         goto    DebounceState1  ; If SW2 is pressed again in this state then return to State1
001B   1D0B           00107         btfss   INTCON,T0IF     ; Else, continue to count down 16ms
001C   2819           00108         goto    DebounceState2  ; Time = TMR0_max * TMR0 prescaler * (1/(Fosc/4)) = 256*128*0.5E-6 = 16.
                            4ms
001D   2811           00109         goto    Main
                      00110 
