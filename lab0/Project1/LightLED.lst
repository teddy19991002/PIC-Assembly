000D   009C           00075         movwf   CMCON0                  ; turn off comparators
                      00076 ; Turn off Analog 
000E   0191           00077         clrf    ANSEL
                      00078 
000F   1283           00079         bcf             STATUS,RP0              ; go back to bank 0
